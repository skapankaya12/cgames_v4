{
  "version": 2,
  "installCommand": "npm install",
  "functions": {
    "api/*.ts": {
      "runtime": "@vercel/node@2.6.0"
    },
    "api/*.js": {
      "runtime": "@vercel/node@2.6.0"
    },
    "ping.js": {
      "runtime": "@vercel/node@2.6.0"
    }
  },
  "routes": [
    {
      "src": "/ping",
      "dest": "/ping.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 