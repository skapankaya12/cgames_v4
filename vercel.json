{
  "version": 2,
  "functions": {
    "api/*.ts": {
      "runtime": "@vercel/node@2.6.0"
    },
    "test.js": {
      "runtime": "@vercel/node@2.6.0"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/test",
      "dest": "/test.js"
    },
    {
      "src": "/(.*)",
      "dest": "/apps/hr-platform/dist/$1"
    }
  ]
} 