{
  "version": 2,
  "buildCommand": "npm run build:hr",
  "outputDirectory": "apps/hr-platform/dist",
  "installCommand": "npm install",
  "functions": {
    "api/*.ts": {
      "runtime": "@vercel/node@2.6.0"
    },
    "api/*.js": {
      "runtime": "@vercel/node@2.6.0"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 