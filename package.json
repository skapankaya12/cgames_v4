{
  "name": "cgames-v4-monorepo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "npm@10.9.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "clean": "turbo run clean",
    "dev:hr": "turbo run dev --filter=hr-platform",
    "dev:game": "turbo run dev --filter=game-platform",
    "build:hr": "turbo run build --filter=hr-platform",
    "build:game": "turbo run build --filter=game-platform",
    "vercel-build": "npm run build:hr",
    "ngrok": "ngrok http 5173"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.9.0",
    "dotenv": "^16.5.0",
    "firebase": "^11.8.1",
    "google-spreadsheet": "^4.1.4",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "openai": "^4.103.0",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^5.2.133",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.1",
    "recharts": "^2.12.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/pdf-parse": "^1.1.5",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "ngrok": "^5.0.0-beta.2",
    "turbo": "^2.3.3",
    "typescript": "latest",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
}
